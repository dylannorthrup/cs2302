package objectoriented;

public class BirthDate {
	private int year;
	private int month;
	private int day;
	
	public BirthDate(int y, int m, int d) {
		year = y;
		month = m;
		day = d;
	}
	
	public void setYear(int newYear) {
		year = newYear;
	}
	
	public String toString() {
		return month + "/" + day + "/" + year;
	}
}
