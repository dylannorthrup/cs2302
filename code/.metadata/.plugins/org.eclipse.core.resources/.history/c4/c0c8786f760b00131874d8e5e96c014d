package objectoriented;

public class BirthDate {
	private int year;
	private int month;
	private int day;
	
	public BirthDate(int y, int m, int d) {
		this.year = y;
		this.month = m;
		this.day = d;
	}
	
	public void setYear(int newYear) {
		this.year = newYear;
	}
	
	public void setMonth(int month) {
		if (month > 0 && month <= 12)
			this.month = month;
	}
	
	public String toString() {
		return this.month + "/" + this.day + "/" + this.year;
	}
}
